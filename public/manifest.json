{
  "name": "Production Management",
  "author": "Ansuman Swain",
  "version": "0.1.0",
  "description": "You can add Todos,Bookmars",
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_icon": {
      "16": "icons/clock16.png",
      "48": "icons/clock48.png",
      "128": "icons/clock128.png"
    },
    "default_title": "Production Management"
  },
  "icons": {
    "16": "icons/clock16.png",
    "48": "icons/clock48.png",
    "128": "icons/clock128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 3
}